@model IEnumerable<YueShanp.Models.DeliveryOrder>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>DeliveryOrderList</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DeliveryOrderNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CustomerSONumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeliveryOrderDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ReceivableMonth)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Creator)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreateTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastEditor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastEditTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EntityStatus)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DeliveryOrderNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CustomerSONumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DeliveryOrderDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ReceivableMonth)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Creator)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreateTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastEditor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastEditTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EntityStatus)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
                @Html.ActionLink("Details", "Details", new { id=item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.Id })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
